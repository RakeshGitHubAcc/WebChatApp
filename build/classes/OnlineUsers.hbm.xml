<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.webchatapp.hibernatebeans.OnlineUsers" table="onlineusers">
		<meta attribute="class-description">
			This class contains online users and their status.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="userId" column="userId" type="int" />
		<property name="liveStatus" column="liveStatus" type="int" />
	</class>
</hibernate-mapping>